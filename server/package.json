{
  "name": "partmin-server",
  "version": "1.0.0",
  "main": "ts/index.ts",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@puppeteer/browsers": "^1.9.1",
    "axios": "^1.3.3",
    "chromedriver": "^120.0.0",
    "discord.js": "^14.14.1",
    "dotenv-mono": "^1.3.13",
    "haversine": "^1.1.1",
    "he": "^1.2.0",
    "rss-parser": "^3.12.0",
    "runtypes": "^6.7.0",
    "selenium-webdriver": "^4.8.1",
    "single-line-log": "^1.1.2"
  },
  "scripts": {
    "start": "tsc --noEmit && tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "circles": "cd py && python reverse_geocode.py"
  },
  "devDependencies": {
    "@types/chromedriver": "^81.0.1",
    "@types/haversine": "^1.1.8",
    "@types/he": "^1.2.0",
    "@types/selenium-webdriver": "^4.1.11",
    "@types/single-line-log": "^1.1.0",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^2.8.4",
    "tsx": "^4.6.1",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "unused-imports"
    ],
    "extends": [
      "prettier"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts",
          "**/*.tsx"
        ],
        "rules": {
          "unused-imports/no-unused-imports": "error",
          "require-await": "warn",
          "no-multiple-empty-lines": [
            "error",
            {
              "max": 1
            }
          ],
          "no-restricted-imports": [
            "error",
            {
              "patterns": [
                ".*"
              ]
            }
          ]
        },
        "parserOptions": {
          "project": [
            "./tsconfig.json"
          ]
        }
      },
      {
        "files": [
          "config.ts"
        ],
        "rules": {
          "no-restricted-imports": "off"
        }
      }
    ],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "env": {
      "es6": true
    }
  }
}
