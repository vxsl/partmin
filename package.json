  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@types/chromedriver": "^81.0.1",
    "@types/haversine": "^1.1.8",
    "@types/he": "^1.2.0",
    "@types/selenium-webdriver": "^4.1.11",
    "@types/single-line-log": "^1.1.0",
    "axios": "^1.3.3",
    "chromedriver": "^120.0.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.2",
    "form-data": "^4.0.0",
    "geckodriver": "^3.2.0",
    "haversine": "^1.1.1",
    "he": "^1.2.0",
    "rss-parser": "^3.12.0",
    "selenium-webdriver": "^4.8.1",
    "single-line-log": "^1.1.2",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "tsx ts/src/index.ts",
    "circles": "cd py && python reverse_geocode.py"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^2.8.4",
    "tsx": "^4.6.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts",
          "**/*.tsx"
        ],
        "rules": {
          "require-await": "warn",
          "no-console": "warn",
          "no-multiple-empty-lines": [
            "error",
            {
              "max": 1
            }
          ]
        },
        "parserOptions": {
          "project": [
            "./ts/tsconfig.json"
          ]
        }
      }
    ],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "env": {
      "es6": true
    }
  }
}
